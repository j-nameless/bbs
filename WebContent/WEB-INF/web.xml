<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>bbs</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>ControlArticle</servlet-name>
  	<servlet-class>myservlet.ControlArticle</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControlArticle</servlet-name>
  	<url-pattern>/article</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ControlShow</servlet-name>
  	<servlet-class>myservlet.ControlShow</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControlShow</servlet-name>
  	<url-pattern>/show</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ControlReply</servlet-name>
  	<servlet-class>myservlet.ControlReply</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControlReply</servlet-name>
  	<url-pattern>/reply</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ControlPost</servlet-name>
  	<servlet-class>myservlet.ControlPost</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControlPost</servlet-name>
  	<url-pattern>/post</url-pattern>
  </servlet-mapping>
  </web-app>